		<key id="KeyNUMPAD9"	mod="" />
		<key id="KeyMULTIPLY"	mod="" />
		<key id="KeyADD"		mod="" />
		<key id="KeySEPARATOR"	mod="" />
		<key id="KeySUBTRACT"	mod="" />
		<key id="KeyDECIMAL"	mod="" />
		<key id="KeyDIVIDE"		mod="" />
		<key id="KeyNUMLOCK"	mod="" />

		<!-- France. Extra Keys (comma etc...) -->
		<key id="0xBA"			mod="CSA" />
